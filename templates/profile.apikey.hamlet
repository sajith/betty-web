<div #token-panel .panel .panel-default>
  <div .panel-heading>
    <i .fa .fa-key .fa-fw>
    API Token
  <div .panel-body>
    <div #token-update>
      <!-- TODO: use jquery to reset token -->
      <form #token-form .form-horizontal role=form method=post action=@{ResetTokenR}>
        <div .form-group>
          <label .col-sm-4 .control-label>
            #{token}
          <div .col-sm-8>
            <input type=submit value="Reset token" .btn .btn-default>  
